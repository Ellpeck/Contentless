{
    "exclude": [
        "obj/",
        "bin/",
        "Ex*.png",
        "Tiled/*.png"
    ],
    "logSkipped": true,
    "references": ["MonoGame.Extended.Content.Pipeline"],
    "overrides": {
        "Copy.*": {
            "copy": true
        },
        ".json": {
            "copy": true
        },
        ".ogg": {
            "importer": "OggImporter",
            "processor": "SongProcessor"
        },
        ".png": {
            "processorParams": {
                "TextureFormat": "NoChange"
            }
        }
    }
}
