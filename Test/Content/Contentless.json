{
    "exclude": [
        "obj/",
        "bin/",
        "Ex*.png"
    ],
    "logSkipped": true,
    "references": ["monogame.extended.content.pipeline"],
    "overrides": {
        "Copy.*": {
            "copy": true
        },
        ".json": {
            "importer": "JsonContentImporter",
            "processor": "FontTextureProcessor"
        },
        ".ogg": {
            "importer": "OggImporter",
            "processor": "SongProcessor"
        },
        ".xml": {
            "processor": "FontTextureProcessor"
        },
        ".png": {
            "processorParams": {
                "TextureFormat": "Compressed"
            }
        }
    }
}
